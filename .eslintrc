{
    "rules": {
        "indent": [2, 2],
        "quotes": [2, "single"],
        "linebreak-style": [2, "unix"],
        "semi": [2, "always"],
        "curly": 2,
        "arrow-spacing": 2,
        "prefer-arrow-callback": 2,
        "prefer-const": 2,
        "angular/no-service-method": 0
    },
    "env": {
        "es6": true,
        "browser": true,
        "commonjs": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "modules": true
    },
    "plugins": [
        "angular"
    ]
}
